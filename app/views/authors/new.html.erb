<h1>New Author</h1>
<% if @author.errors.any? %>
  <ul><% @author.errors.full_messages.each do |message| %>
  <li><%= message %></li>
  <% end %>
</ul>
<% end %>

<%= form_for @author do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br></br>

<%= f.label :birth_year %>
<%= f.number_field :birth_year %>
<br></br>

<%= f.submit "Create Author" %>
<% end %>
